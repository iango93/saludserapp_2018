<p id="notice"><%= notice %></p>

<h1>Accountnames</h1>

<table>
  <thead>
    <tr>
      <th>Type account</th>
      <th>Address</th>
      <th>Identification</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accountnames.each do |accountname| %>
      <tr>
        <td><%= accountname.type_account %></td>
        <td><%= accountname.address %></td>
        <td><%= accountname.identification %></td>
        <td><%= accountname.email %></td>
        <td><%= link_to 'Show', accountname %></td>
        <td><%= link_to 'Edit', edit_accountname_path(accountname) %></td>
        <td><%= link_to 'Destroy', accountname, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Accountname', new_accountname_path %>
