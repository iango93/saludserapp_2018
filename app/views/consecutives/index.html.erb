<p id="notice"><%= notice %></p>

<h1>Consecutives</h1>

<table>
  <thead>
    <tr>
      <th>Suffix</th>
      <th>Folio company</th>
      <th>Company</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @consecutives.each do |consecutive| %>
      <tr>
        <td><%= consecutive.suffix %></td>
        <td><%= consecutive.folio_company %></td>
        <td><%= consecutive.company %></td>
        <td><%= consecutive.customer %></td>
        <td><%= link_to 'Show', consecutive %></td>
        <td><%= link_to 'Edit', edit_consecutive_path(consecutive) %></td>
        <td><%= link_to 'Destroy', consecutive, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Consecutive', new_consecutive_path %>
